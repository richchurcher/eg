{
  "name": "eg",
  "version": "0.1.0",
  "description": "Eloquent Geek",
  "author": "Rich Churcher <rich.churcher@gmail.com>",
  "private": true,
  "scripts": {
    "build": "node build/build.js",
    "client": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "e2e": "node test/e2e/runner.js",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "knex": "knex",
    "server": "pm2 start egserver.config.js",
    "start": "npm run server && npm run client",
    "start:server": "npm run transpile && cd server && node --require 'dotenv/config' dist/index.js",
    "stop": "pm2 delete egserver",
    "test": "npm run unit && npm run e2e",
    "transpile": "babel server/src --out-dir server/dist",
    "unit": "jest --config test/unit/jest.conf.js --coverage"
  },
